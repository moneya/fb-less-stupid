{
  "name": "Facebook Less Stupid",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Removes Facebook posts that contain terms indicating the Facebook post is vapid linkbait.",
  "homepage_url": "http://ianww.com",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "options_page": "src/options_custom/index.html",
  "permissions": [
    "https://*.facebook.com/*",
    "http://*.facebook.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*.facebook.com/*",
        "http://*.facebook.com/*"
      ],
      "js": [
        "src/inject/inject.js"
      ]
    }
  ]
}
